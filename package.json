    "@storybook/builder-vite": "^7.0.22",
    "@storybook/react": "^7.0.21",
    "@storybook/react-webpack5": "^7.0.22",
    "@storybook/testing-library": "^0.0.14-next.2",
    "esbuild": "^0.18.4",
    "prop-types": "^15.8.1",
    "rollup": "^3.25.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "storybook": "^7.0.21",
    "styled-components": "^6.0.0-rc.3"
  },
  "dependencies": {
